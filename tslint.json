{
    "rules": {
        "class-name": true,
        "no-duplicate-variable": true,
        "no-eval": true,
        "no-internal-module": true,
        "no-unsafe-finally": true,
        "no-floating-promises": true,
        "await-promise": true,
        "variable-name": [
            true,
            "ban-keywords"
        ],
        "no-console": [
            true,
            "log", "error"
        ]
    }
}
